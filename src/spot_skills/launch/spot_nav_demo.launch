<!-- Launch the nodes needed to demonstrate SLAM + navigation on Spot, using Spot_ROS -->
<launch>

    <!-- For safety reasons, do not assume which Spot is being used -->
    <arg name="spot_name" />

    <include file="$(find spot_skills)/launch/spot_slam_demo.launch">
    </include>

    <!-- Launch Spot's navigation stack from spot_move_base -->
    <include file="$(find spot_move_base)/launch/move_base.launch"/>

</launch>