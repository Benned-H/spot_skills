{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Source ROS and Profile TMP.py",
            "type": "shell",
            "command": "/bin/bash",
            "args": [
                "-c",
                "source devel/setup.bash && austin -h 10 python3 src/tmp3/TMP.py"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$python"
            ],
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared"
            }
        }
    ]
}